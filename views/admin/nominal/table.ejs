<table id="example2" class="table table-bordered table-hover">
  <thead>
    <tr>
      <th>No.</th>
      <th>Nama Koin</th>
      <th>Jumlah Koin</th>
      <th>Harga</th>
      <th>Aksi</th>
    </tr>
  </thead>
  <tbody>
    <% nominals.forEach((nominal, i)=> { %>
      <tr>
        <td>
          <%= ++i %>
        </td>
        <td>
          <%= nominal.coinName %>
        </td>
        <td>
          <%= nominal.coinQuantity %>
        </td>
        <td>
          <%= nominal.price %>
        </td>
        <td>
          <form action="/nominal/<%= nominal._id %>?_method=DELETE" method="post">
            <a href="/nominal/<%= nominal._id %>/edit" class="btn btn-info btn-sm">
              <i class="nav-icon fas fa-pen"></i>
            </a>
            <button type="submit" class="btn btn-danger btn-sm"><i class="nav-icon fas fa-trash"></i></button>
          </form>
        </td>
      </tr>
      <% }) %>
  </tbody>
</table>